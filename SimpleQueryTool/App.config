<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings configSource="Connections.config"/>
  
  <!--
  Put a file named 'Connections.config' with the following content next to your executable.
  <?xml version="1.0" encoding="utf-8" ?>
  <connectionStrings>
    <clear />
    <add name="Any name" providerName="Your database provider name" connectionString="Whatever your provider needs to connect your DB." />
  </connectionStrings>
  -->

  <system.data>
    <DbProviderFactories>
      <add name="PostgreSQL Data Provider" invariant="Npgsql" description=".Net Framework Data Provider for PostgreSQL" type="Npgsql.NpgsqlFactory, Npgsql, Version=3.2.5.0, Culture=neutral, PublicKeyToken=5d8b90d52f46fda7" />
    </DbProviderFactories>
  </system.data>
</configuration>
